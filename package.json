{
  "name": "",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --mode dev",
    "stage": "astro dev --mode stage",
    "prod": "astro dev --mode prod",
    "build": "astro check && astro build --mode prod",
    "preview": "astro preview --mode stage",
    "astro": "astro",
    "prepare": "husky",
    "test": "jest",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "disableScopeLowerCase": false,
      "disableSubjectLowerCase": false,
      "maxHeaderWidth": 100,
      "maxLineWidth": 100,
      "defaultType": "",
      "defaultScope": "",
      "defaultSubject": "",
      "defaultBody": "",
      "defaultIssues": "",
      "types": {
        "feat": {
          "description": "A new feature",
          "title": "Features",
          "emoji": "‚ú®"
        },
        "fix": {
          "description": "A bug fix",
          "title": "Bug Fixes",
          "emoji": "üêõ"
        },
        "refactor": {
          "description": "A code change that neither fixes a bug nor adds a feature",
          "title": "Code Refactoring",
          "emoji": "üì¶"
        },
        "test": {
          "description": "Adding missing tests or correcting existing tests",
          "title": "Test",
          "emoji": "üö®"
        },
        "chore": {
          "description": "Other changes that don't modify src or test files",
          "title": "Chores",
          "emoji": "‚ôªÔ∏è"
        }
      }
    }
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/db": "^0.14.4",
    "@astrojs/node": "^9.0.0",
    "@astrojs/preact": "^4.0.0",
    "@astrojs/tailwind": "^5.1.4",
    "@astrojs/vercel": "^8.0.1",
    "@auth/core": "^0.37.4",
    "astro": "^5.0.5",
    "auth-astro": "^4.2.0",
    "cloudinary": "^2.5.1",
    "node-cron": "^3.0.3",
    "preact": "^10.24.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@jest/globals": "^29.7.0",
    "@types/node-cron": "^3.0.11",
    "commitizen": "^4.3.1",
    "husky": "^9.1.6",
    "inquirer": "^9.3.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5"
  }
