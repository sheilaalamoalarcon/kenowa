{
  "name": "kenowa",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --mode dev",
    "stage": "astro dev --mode stage",
    "prod": "astro dev --mode prod",
    "build": "astro check && astro build --mode prod",
    "preview": "astro preview --mode stage",
    "astro": "astro",
    "prepare": "husky",
    "test": "playwright test",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "disableScopeLowerCase": false,
      "disableSubjectLowerCase": false,
      "maxHeaderWidth": 100,
      "maxLineWidth": 100,
      "defaultType": "",
      "defaultScope": "",
      "defaultSubject": "",
      "defaultBody": "",
      "defaultIssues": "",
      "types": {
        "feat": {
          "description": "A new feature",
          "title": "Features",
          "emoji": "‚ú®"
        },
        "fix": {
          "description": "A bug fix",
          "title": "Bug Fixes",
          "emoji": "üêõ"
        },
        "refactor": {
          "description": "A code change that neither fixes a bug nor adds a feature",
          "title": "Code Refactoring",
          "emoji": "üì¶"
        },
        "test": {
          "description": "Adding missing tests or correcting existing tests",
          "title": "Test",
          "emoji": "üö®"
        },
        "chore": {
          "description": "Other changes that don't modify src or test files",
          "title": "Chores",
          "emoji": "‚ôªÔ∏è"
        }
      }
    }
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/db": "^0.14.7",
    "@astrojs/node": "^9.1.2",
    "@astrojs/preact": "^4.0.5",
    "@astrojs/vercel": "^8.1.0",
    "@auth/core": "^0.37.4",
    "@tailwindcss/vite": "^4.0.9",
    "astro": "^5.4.1",
    "auth-astro": "^4.2.0",
    "cloudinary": "^2.5.1",
    "node-cron": "^3.0.3",
    "preact": "^10.24.3",
    "tailwindcss": "^4.0.9",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.49.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/preact": "^3.2.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/testing-library__jest-dom": "^6.0.0",
    "commitizen": "^4.3.1",
    "husky": "^9.1.6",
    "inquirer": "^9.3.7",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^25.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
