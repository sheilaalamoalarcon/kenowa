{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "verbatimModuleSyntax": false,
    "types": [
      "jest",
      "@testing-library/jest-dom"
    ],
    "module": "ESNext", // or ES2022
    "target": "ESNext",
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "*"
      ],
      "@/Components": [
        "Components/*"
      ],
      "@/Constants": [
        "Constants/*"
      ],
      "@/i18n": [
        "i18n/*"
      ],
      "@/Layouts": [
        "Layouts/*"
      ],
      "@/Pages": [
        "Pages/*"
      ],
      "@/Schemas": [
        "Schemas/*"
      ],
      "@/Styles": [
        "Styles/*"
      ],
    }
  }
}